# Main MySQL / Maria DB
DIVVY_MYSQL_HOST=your.mysql.server.host.name.com
DIVVY_MYSQL_PORT=3306
DIVVY_MYSQL_USER=db_username
DIVVY_MYSQL_PASSWORD=dbpassword

# Secure db - Stores encryption keys and is only accessible via Worker processes
DIVVY_SECURE_MYSQL_HOST=your.secure.mysql.server.com
DIVVY_SECURE_MYSQL_PORT=3306
DIVVY_SECURE_MYSQL_USER=secure_db_username
DIVVY_SECURE_MYSQL_PASSWORD=securedbpassword

# Redis
DIVVY_REDIS_HOST=your.redis.host.name
DIVVY_REDIS_PORT=6379

# ElasticSearch
DIVVY_ES_HOST=http://your_elastic_search_url
DIVVY_ES_PORT=9200

# Divvy Required - do not modify
VIRTUAL_ENV=/
VIRTUAL_HOST="*"
DIVVY_MYSQL_DB_NAME=divvy 
DIVVY_SECURE_MYSQL_DB_NAME=divvy_keys 
